{
  "swagger": "2.0",
  "info": {
    "version": "1.0.0",
    "title": "DnD Companion Tool Swagger API",
    "description": "A swagger API for the dnd companion tool"
  },
  "host": "dndCompTool.swagger.io",
  "basePath": "/api",
  "schemes": [
    "http"
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/characters": {
      "get": {
        "description": "Returns all characters that the user has access to.",
        "operationId": "findCharacters",
        "parameters": [
          {
            "name": "tags",
            "in": "query",
            "description": "tags to filter by",
            "required": false,
            "type": "array",
            "collectionFormat": "csv",
            "items": {
              "type": "string"
            }
          },
          {
            "name": "limit",
            "in": "query",
            "description": "maximum number of results to return",
            "required": false,
            "type": "integer",
            "format": "int32"
          }
        ],
        "responses": {
          "200": {
            "description": "character response",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Character"
              }
            }
          },
          "403": {
            "description": "no characters found",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Error"
              }
            }
          },
          "default": {
            "description": "unexpected error",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          }
        }
      },
      "post": {
        "description": "Creates a new character in the store.  Duplicates are allowed",
        "operationId": "addCharacter",
        "parameters": [
          {
            "name": "character",
            "in": "body",
            "description": "Character to add to the users collection",
            "required": true,
            "schema": {
              "$ref": "#/definitions/NewCharacter"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "character response",
            "schema": {
              "$ref": "#/definitions/Character"
            }
          },
          "default": {
            "description": "unexpected error",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          }
        }
      }
    },
    "/characters/{id}": {
      "get": {
        "description": "Returns a character based on a single ID",
        "operationId": "find a character by id",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "ID of character to fetch",
            "required": true,
            "type": "integer",
            "format": "int64"
          }
        ],
        "responses": {
          "200": {
            "description": "character response",
            "schema": {
              "$ref": "#/definitions/Character"
            }
          },
          "403": {
            "description": "no character found with this id",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Error"
              }
            }
          },
          "default": {
            "description": "unexpected error",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          }
        }
      },
      "delete": {
        "description": "deletes a single character based on the ID supplied",
        "operationId": "deleteCharacter",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "ID of character to delete",
            "required": true,
            "type": "integer",
            "format": "int64"
          }
        ],
        "responses": {
          "204": {
            "description": "character deleted"
          },
          "403": {
            "description": "no character found with this id",
            "schema": {
              "items": {
                "$ref": "#/definitions/Error"
              }
            }
          },
          "default": {
            "description": "unexpected error",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          }
        }
      }
    },
    "/cs/{id}": {
        "get": {
          "description": "",
          "operationId": "",
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "description": "ID of character to fetch",
              "required": true,
              "type": "integer",
              "format": "int64"
            }
          ],
          "responses": {
            "200": {
              "description": "",
              "schema": {
                "$ref": "#/definitions/Character"
              }
            },
            "403": {
              "description": "",
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/definitions/Error"
                }
              }
            },
            "default": {
              "description": "unexpected error",
              "schema": {
                "$ref": "#/definitions/Error"
              }
            }
          }
        },
        "delete": {
          "description": "",
          "operationId": "",
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "description": "",
              "required": true,
              "type": "integer",
              "format": "int64"
            }
          ],
          "responses": {
            "204": {
              "description": ""
            },
            "403": {
              "description": "",
              "schema": {
                "items": {
                  "$ref": "#/definitions/Error"
                }
              }
            },
            "default": {
              "description": "unexpected error",
              "schema": {
                "$ref": "#/definitions/Error"
              }
            }
          }
        }
      }
  },
  "definitions": {
    "Character": {
      "type": "object",
      "allOf": [
        {
          "$ref": "#/definitions/NewCharacter"
        },
        {
          "required": [
            "id"
          ],
          "properties": {
            "id": {
              "type": "integer",
              "format": "int64"
            }
          }
        }
      ]
    },
    "NewCharacter": {
      "type": "object",
      "required": [
        "name"
      ],
      "properties": {
        "name": {
          "type": "string"
        },
        "tag": {
          "type": "string"
        }
      }
    },
    "Error": {
      "type": "object",
      "required": [
        "code",
        "message"
      ],
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        }
      }
    }
  }
}